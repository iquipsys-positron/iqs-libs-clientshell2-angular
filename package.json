        "test:lib": "ng test iqs-libs-clientshell2-angular",
        "lint": "ng lint",
        "lint:lib": "ng lint iqs-libs-clientshell2-angular",
        "e2e": "ng e2e",
        "package": "npm run build:lib && (for %I in (README.md CHANGELOG.md) do copy .\\%I .\\dist\\iqs-libs-clientshell2-angular) && cd dist/iqs-libs-clientshell2-angular && npm pack"
    },
